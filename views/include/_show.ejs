<% for(var i = 0; i < accounts.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= accounts[i].account%></td>
        <td><%= accounts[i].password %></td>
        <td><%= accounts[i].type %></td>
        <td><%= accounts[i].status %></td>
        <td>
            <a href="/update-account?id=<%= accounts[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= accounts[i]._id%> >
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>